{
  "name": "expense-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "auth:generate": "pnpm dlx @better-auth/cli generate --output src/db/auth-schema.ts -y",
    "build": "next typegen && eslint && next build --turbopack",
    "db:generate": "drizzle-kit generate --config=./src/db/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./src/db/drizzle.config.ts",
    "dev": "next dev --turbopack",
    "duplicate:check": "jscpd ./src --pattern \"**/*.{ts,tsx}\"",
    "email:dev": "email dev --dir src/emails --port 3001",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "eslint",
    "lint:report": "ts-node ./src/scripts/eslint-out.ts",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "unused": "knip",
    "update:check": "npm-check-updates"
  },
